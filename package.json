{
  "name": "react-redux-server-template",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bookshelf": "^0.10.4",
    "classnames": "^2.2.5",
    "ejs": "^2.5.7",
    "knex": "^0.13.0",
    "nconf": "^0.8.5",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-scripts": "1.0.14",
    "redux": "^3.7.2",
    "validator": "^9.0.0"
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "build": "npm run clean && webpack --progress --bail --env dist -p",
    "lint": "eslint ./src",
    "lint-fix": "eslint ./src --fix",
    "lint-sass": "sass-lint -c .sass-lint.yml 'src/styles/**/*.scss' -v -q",
    "posttest": "npm run lint",
    "serve:dev": "webpack-dev-server --env dev --hot --history-api-fallback",
    "serve:dist": "webpack-dev-server --env dist -p --progress",
    "start": "webpack --progress --watch & nodemon dist/server --exec babel-node"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "copy-webpack-plugin": "^4.2.0",
    "eslint": "^4.9.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "jsx-loader": "^0.13.2",
    "node-babel": "^0.1.2",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.1",
    "rimraf": "^2.6.2",
    "sass-lint": "^1.12.1",
    "sass-loader": "^6.0.6",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3",
    "webpack-node-externals": "^1.6.0"
  }
}
